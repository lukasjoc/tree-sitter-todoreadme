{
  "name": "todoreadme",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "action"
      }
    },
    "action": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "single_line_action"
        }
      ]
    },
    "single_line_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "current|task|idea|later"
    },
    "text": {
      "type": "PATTERN",
      "value": ".*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

